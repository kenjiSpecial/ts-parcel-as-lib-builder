{
	"name": "ts-parcel-as-lib-builder",
	"version": "0.0.0",
	"description": "Example project: Parcel as js Library builder",
	"main": "src/index.ts",
	"author": "satotake",
	"license": "MIT",
	"scripts": {
		"build": "parcel build src/index.ts --global FOUR",
		"doc": "typedoc --out docs --theme minimal --mode file src",
		"start": "rm -rf dist/*.js dist/*.js.map && parcel serve dev/index.html --no-hmr --port 1234",
		"lint": "tslint -t stylish --project \"tsconfig.json\""
	},
	"dependencies": {
		"dan-shari-gl": "^1.3.2",
		"dat.gui": "^0.7.6",
		"gl-matrix": "^3.1.0",
		"gsap": "^3.0.4",
		"stats-js": "^1.0.1",
		"stats.js": "^0.17.0"
	},
	"devDependencies": {
		"@types/gl-matrix": "^2.4.5",
		"glslify-bundle": "^5.1.1",
		"glslify-deps": "^1.3.1",
		"parcel-bundler": "^1.12.4",
		"prettier": "^1.19.1",
		"tslint": "^5.20.1",
		"tslint-config-prettier": "^1.18.0",
		"tslint-microsoft-contrib": "^6.2.0",
		"tsutils": "^3.17.1",
		"typedoc": "^0.15.5",
		"typescript": "^3.7.3"
	}
}

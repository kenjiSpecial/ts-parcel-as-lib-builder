		"parcel-bundler": "^1.11.0",
		"prettier": "^1.16.3",
		"tslint": "^5.12.1",
		"tslint-config-prettier": "^1.17.0",
		"tslint-microsoft-contrib": "^6.0.0",
		"tsutils": "^3.8.0",
		"typedoc": "^0.14.2",
		"typescript": "^3.2.4"
	}
